%
% Roumanian characters:
%
% 'A for \u{a}
% 'a for \u{a}
% 'S for \c{S}
% 's for \c{s}
% 'T for \c{T}
% 't for \c{t}
% 'I for \^I
% 'i for \^\i
%
% CAUTION! ' is now an active character, so it can't be used to obtain the
% sign '. Use insteed '".
%
% \' has the old meaning.
%
\def\myqq@{'}
\catcode`\'=13 
\def'#1{\ifmmode {}^\prime #1%
         \else\ifx#1a\u{a}%
         \else\ifx#1i\^{\i}%
         \else\ifx#1s\c{s}%
     	 \else\ifx#1t\c{t}%
     	 \else\ifx#1A\u{A}%
     	 \else\ifx#1I\^{I}%
     	 \else\ifx#1S\c{S}%
         \else\ifx#1T\c{T}%
         \else\ifx#1"\myqq@%
               \fi\fi\fi\fi\fi\fi\fi\fi\fi%
         \fi}  
